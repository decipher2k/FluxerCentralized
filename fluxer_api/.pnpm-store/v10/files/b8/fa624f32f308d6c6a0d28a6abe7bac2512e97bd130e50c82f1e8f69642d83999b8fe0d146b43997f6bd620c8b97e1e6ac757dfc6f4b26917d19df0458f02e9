import { Cid } from './cid.js';
export type BlobRef = {
    $type: 'blob';
    mimeType: string;
    ref: Cid;
    size: number;
};
export declare function isBlobRef(input: unknown, options?: {
    strict?: boolean;
}): input is BlobRef;
export type LegacyBlobRef = {
    cid: string;
    mimeType: string;
};
export declare function isLegacyBlobRef(input: unknown): input is LegacyBlobRef;
